- titulo t(:propiedades_datos)

= link_to t(:agregar_, item: t(:datos_productivos).downcase), new_admin_dato_recolectado_path, class: 'btn btn-sm btn-primary margin-bottom-20 btn-sm'
.card
  .card-header
    %h5.no-margin
      = t(:buscar_, item: t(:propiedades).downcase)
  .card-body
    #establecimientos-remote-filter
      = render 'filtros'

.card
  .card-header
    %h5.no-margin
      = t(:lista_de_, items: t(:propiedades).downcase)
  .card-body
    #establecimientos-remote-table
      = render 'table'