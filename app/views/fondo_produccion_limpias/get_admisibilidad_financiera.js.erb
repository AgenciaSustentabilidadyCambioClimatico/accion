    var nota = '<%= cuestionario.nota %>'; // Asumiendo que justificacion es el nombre del atributo
    if (nota < 5) { // Se debe comparar justificacion con un nÃºmero, no con una cadena
        //console.log("Valor en justificacion <%= index + 1 %>, <%= cuestionario.justificacion %>");
        $('#obs_input_id_<%= index + 1 %>').css('visibility', 'visible'); // Cambiamos la visibilidad del campo
        $('#obs_input_id_<%= index + 1 %>').val('<%= cuestionario.justificacion %>'); // Establecemos su valor
    }
<% end %>

