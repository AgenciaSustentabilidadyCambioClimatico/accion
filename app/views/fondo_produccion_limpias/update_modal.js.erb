    $consultor.removeClass('btn-danger').addClass('btn-primary');
    $consultor.attr('data-tooltip', 'false');

    $('#div_empresa_consultora table tbody').empty();
    <% @contribuyente.each do |contribuyente| %>
        var razonSocial = '<%= j contribuyente.razon_social %>';
        var rut = '<%= format_rut("#{contribuyente.rut || ''}#{contribuyente.dv || ''}") %>';

        var newRow = "<tr>" +
        "<td class='contenido-form'>" + razonSocial + "</td>" +
        "<td class='contenido-form'>" + rut + "</td>" +
        "<td class='contenido-form dos iconos-acciones'>" +
        "<i class='fa fa-exclamation-circle' data-toggle='tooltip' title='La empresa no puede ser eliminada ya que tiene un equipo de consultores asociados.'></i>" +
        "</td>" +
        "</tr>";    
        $('#div_empresa_consultora table tbody').append(newRow); 
    <% end %>
<% end %>

