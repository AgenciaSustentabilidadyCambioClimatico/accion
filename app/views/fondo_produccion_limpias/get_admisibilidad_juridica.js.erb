<% cuestionario_fpl.each_with_index do |cuestionario, index| %>
    //console.log("Valor en nota #nota_input_juridica_id_<%= cuestionario.criterio_id %>, <%= cuestionario.nota %>");
    $('#nota_input_juridica_id_<%= cuestionario.criterio_id %>').val('<%= cuestionario.nota %>');
    var nota = '<%= cuestionario.nota %>'; // Asumiendo que justificacion es el nombre del atributo
    if (nota == 2) { // Se debe comparar justificacion con un n√∫mero, no con una cadena
        //console.log("Valor en justificacion #obs_input_juridica_id_<%= cuestionario.criterio_id %> , <%= cuestionario.justificacion %>");
        $('#obs_input_juridica_id_<%= cuestionario.criterio_id %>').css('visibility', 'visible'); // Cambiamos la visibilidad del campo
        $('#obs_input_juridica_id_<%= cuestionario.criterio_id %>').val('<%= cuestionario.justificacion %>'); // Establecemos su valor
    }
<% end %>

$('#div_equipo_consultora_empresa').css('display', 'block');
