<% cuestionario_fpl.each_with_index do |cuestionario, index| %>
    var nota = document.getElementById('nota_input_obs_id_<%= index + 1 %>');
    var justificacion = document.getElementById('obs_input_obs_id_<%= index + 1 %>');

    nota.textContent = '<%= cuestionario.nota %>';
    justificacion.textContent = '<%= cuestionario.justificacion %>';

    if (<%= cuestionario.nota %> <= 4) {
        nota.classList.add('texto-rojo'); // Agregar la clase si no cumple
    }

<% end %>


