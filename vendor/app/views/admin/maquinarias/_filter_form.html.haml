- es_para_seleccion = es_para_seleccion.nil? ? false : es_para_seleccion
- buscar_por_actividad_economica = buscar_por_actividad_economica.nil? ? false : buscar_por_actividad_economica
- resultado_mostrados = resultado_mostrados.nil? ? 30 : resultado_mostrados

:javascript
  $(document).ready(function() {
    $('.chosen-control').chosen({
      allow_single_deselect: true,
      no_results_text: 'No se encontraron resultados',
      width: '100%'
    });
  });

= render 'admin/maquinarias/resultado_consulta'
= simple_form_for(@contribuyente, url: search_admin_maquinarias_path, remote: true, html: { method: :put, class: 'filter', autocomplete: :off }) do |f| 
  = f.input :es_maquinarias, as: :hidden, input_html: { value: true }, wrapper: false
  - if es_para_seleccion
    = f.input :es_para_seleccion, as: :hidden, input_html: { value: true }, wrapper: false
  - if resultado_mostrados
    = f.input :resultado_mostrados, as: :hidden, input_html: { value: resultado_mostrados }, wrapper: false
  .row
    .col-lg-2.pr-lg-0.col-12
      = f.input :rut, label: false, input_html: { class: 'not-required-if-other-like-me-is-present form-control-sm m-0' }, placeholder: "R.U.T (sin DV) de la instituci처n", as: :string, required: false
    .col-12.col-lg-3
      = f.input :razon_social, input_html: { class: 'not-required-if-other-like-me-is-present form-control-sm m-0', type: :string }, label: false, placeholder: 'Raz처n social de la instituci처n', required: false
    .col-12.col-lg-2
      = f.input :nombre_maquinaria, input_html: { class: 'not-required-if-other-like-me-is-present form-control-sm m-0', type: :string }, label: false, placeholder: 'Nombre de la maquinaria', required: false
    .col-12.col-lg-2
      = f.input :numero_serie, input_html: { class: 'not-required-if-other-like-me-is-present form-control-sm m-0', type: :string }, label: false, placeholder: 'N첫mero de serie', required: false
    .col-12.col-lg-2
      = f.input :patente, input_html: { class: 'not-required-if-other-like-me-is-present form-control-sm m-0', type: :string }, label: false, placeholder: 'Patente', required: false
    .col-lg-1.col-12
      = button_tag "<i class='fa fa-search fa-2x'></i>".html_safe, 'data-disable-with' => "<i class='fa fa-spinner fa-spin fa-2x'></i>".html_safe, id: 'consultar-button', class: "btn btn-sm btn-light btn-block p-0 btn-disabled", disabled: :disabled