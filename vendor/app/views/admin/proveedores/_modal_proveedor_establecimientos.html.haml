.modal.fade{id: :"proveedor-establecimientos", tabindex: "-1", role: :dialog, "aria-labelledby": :proveedorEstablecimientosModalLabel, "aria-hidden": true}
	.modal-dialog.modal-dialog-centered.modal-lg.larger-please.animated{role: :document}
		.modal-content
			.modal-body.no-padding
				.card.no-margin
					.card-header
						%h5.no-margin
							= t(:lista_de_, items: t(:establecimientos).downcase)
					.card-body#proveedor-establecimientos-remote-form
						=# render 'table_establecimientos', record: record, establecimientos: establecimientos, es_para_seleccion: true
			.modal-footer
				%button.btn.btn-secondary{type: :button, "data-dismiss"=>:modal}
					Cerrar