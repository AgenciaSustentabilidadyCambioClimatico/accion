- custom_id = custom_id.blank? ? nil : custom_id
- data_table = data_table.nil? ? true : data_table
-#DZC agregado por nosotros
- es_maquinaria = es_maquinaria.blank? ? false : es_maquinaria
-#DZC
- es_para_seleccion = es_para_seleccion.blank? ? false : es_para_seleccion
- seleccion_basica = seleccion_basica.nil? ? false : seleccion_basica
- buscar_por_actividad_economica = buscar_por_actividad_economica.blank? ? false : buscar_por_actividad_economica
- resultado_mostrados = resultado_mostrados.nil? ? 30 : resultado_mostrados
- tipo_instrumento = tipo_instrumento.nil? ? TipoInstrumento::PPP_EJECUTADOS_POR_TERCEROS : tipo_instrumento

.modal.fade{id: :"buscar-contribuyente#{custom_id}", tabindex: "-1", role: :dialog, "aria-labelledby": :buscarContribuyenteLabel, "aria-hidden": true}
  .modal-dialog.modal-dialog-centered.modal-lg.modal-lg-fix.animated{role: :document}
    .modal-content.p-3
      .modal-body.no-padding
        .card.no-margin
          .card-header
            %h5.no-margin
              = t(:buscar_, item: t(:instituciÃ³n).downcase)
          .card-body
            %div{id: "contribuyentes-remote-filter#{custom_id}"}
              = render 'admin/contribuyentes/filter_form', es_para_seleccion: es_para_seleccion, buscar_por_actividad_economica: buscar_por_actividad_economica, resultado_mostrados: resultado_mostrados, custom_id: custom_id, data_table: data_table, seleccion_basica: seleccion_basica, tipo_instrumento: tipo_instrumento
        %br
        -#
          #resultado-consulta-remote-block
            = render 'admin/contribuyentes/resultado_consulta'

        .card.no-margin
          .card-header
            %h5.no-margin
              = t(:lista_de_, items: t(:contribuyentes).downcase)
          .card-body.p-0
            %div{id: "contribuyentes-remote-table#{custom_id}"}
              = render 'admin/contribuyentes/table', data_table: data_table, es_para_seleccion: es_para_seleccion, resultado_mostrados: resultado_mostrados, seleccion_basica: seleccion_basica, tipo_instrumento: tipo_instrumento
      .modal-footer.pr-0
        %button.btn.btn-secondary{type: :button, "data-dismiss"=>:modal}
          Cerrar
        %button.btn.btn-primary.seleccionar-contribuyente-button{type: :button, "data-dismiss"=>:modal, disabled: :disabled}
          Seleccionar
